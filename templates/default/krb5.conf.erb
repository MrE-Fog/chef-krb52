[libdefaults]
	<% node[:krb5][:options].sort.each do |r, c| -%>
	<%= r %> = <%= c %>
	<% end -%>

[realms]
	<% node[:krb5][:realms].sort.each do |r, c| -%>
	<%= r %> = {
		<% c.sort.each do |opt, val| -%>
			<% val.sort.each do |i| -%>
		<%= opt -%> = <%= i %>
		<% end -%>
		<% end -%>
	}
	<% end -%>
